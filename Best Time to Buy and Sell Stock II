class Solution(object):
    def maxProfit(self, prices):
        """
        :type prices: List[int]
        :rtype: int
        """
        if not prices:
            return 0
        profit=[]
        for i in range(len(prices)-1):
            if prices[i] < prices[i+1]:
                profit.append(prices[i+1]-prices[i])
        return sum(profit)
